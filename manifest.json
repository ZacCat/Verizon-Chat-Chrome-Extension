    "description": "Makes the verizon texting webapp fit the screen",
    "content_scripts": [
        {
            "css": [
                "styles.css"
            ],
            "js": [
                "content.js"
            ],
            "matches": [
                "https://web.vma.vzw.com/vma/web2/Message.do*"
            ]
        }
    ]
}